{
    "debug": true,
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "./log/log",
        "flag": "fatal,error,warn,info,debug,trace",
        "num": 10
    },
    "svr": {
        "bind": [
            "tcp@0.0.0.0:8180"
        ],
        "max_pkg_size": 1024000
    },
    "monitor": {
        "bind": "tcp@127.0.0.1:8181",
        "max_pkg_size": 1024
    },
    "worker_num": 10,
    "timeout": 1.0,
    "mt": {
        "name": "mt",
        "addr": [
            "tcp@127.0.0.1:8190"
        ],
        "max_pkg_size": 1024000
    } 
}
